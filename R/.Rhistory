NumericVector rcpp_seq_rec_minus(NumericVector inits, int w, int n, bool rev) {
NumericVector x(n);
Range idx(0, w - 1);
x[idx] = inits;
if(!rev) {
for (int i = w; i < n; i++){
Range idx(i - w, i - 1);
x[i] = x[i-1] - x[i-2];
}
}
if(rev) {
for (int i = w; i < n; i++){
Range idx(i - w, i - 1);
x[i] = x[i-2] - x[i-1];
}
}
return x;
}
"
)
