if(is.list(x) && length(rep) != 1) rp2 <- as.list(rp)
expect_equal(
sb2_mod(x, i = elements[[i]], rp = rp1),
test_sb(x, i = elements[[i]], rp = rp2)
) |> errorfun()
assign("enumerate", enumerate + 1, envir = parent.frame(n = 1))
}
}
