test_sb <- function(x, i, rp) {
if(!is.list(x)) {
i <- indx_x(i, x, names(x), length(x))
if(length(i) == 0) return(x)
x[i] <- rp
return(x)
}
if(is.list(x)) {
i <- indx_x(i, x, names(x), length(x))
if(length(i) == 0) return(x)
if(length(i) != 1)  x[i] <- as.list(rp)
if(length(i) == 1) x[[i]] <- rp
return(x)
}
}
temp.fun <- function(x, elements) {
